<?xml version="1.0" encoding="utf-8"?>
<set xmlns:android="http://schemas.android.com/apk/res/android">
    <!-- TRANSLATE - Create slide in left animation resource -->
    <!-- X Axis >>>> -100% (left, out Screen) || 0% (onScreen) || 100% (right, out Screen) -->
    <!-- Y Axis >>>> -100% (above Screen) || 0% (onScreen) || 100% (under Screen) -->
    <set xmlns:android="http://schemas.android.com/apk/res/android">
        <translate
            android:duration="@android:integer/config_shortAnimTime"
            android:fromXDelta="-100%"
            android:fromYDelta="0%"
            android:toXDelta="0%"
            android:toYDelta="0%" />
    </set>
</set>